<div class="header" >
  <div class="container">
    <div class="wrapper">
    <div class="row">
      <div class="col-xs-5">
        <div class="logo">
          <h1>Logo Stands Here</h1>
        </div>
      </div>
      <div class="col-xs-7">
        <%= form_for(resource, as: resource_name, url: session_path(resource_name),:html=>{:class=>"form-inline"}) do |f| %>
            <div class="">                                     
                  <%= f.email_field :email,:placeholder=>"Email ",:class=>"form-control", autofocus: true %>
                  <%= f.password_field :password,:placeholder=>"Password",:class=>"form-control", autocomplete: "off" %>
                  <%= f.submit "Log in",:class=>"btn btn-primary" %>
            </div>
                     
        <% end %>
        <% if notice.present? || alert.present? %>
          <div class="notification">
            <p class="notice"><%= notice %></p>
            <p class="alert"><%= alert %></p>
          </div>
        <%end%>
        <%= render "devise/shared/links" %>
      </div>
    </div>                    
    </div>
  </div>
</div>
